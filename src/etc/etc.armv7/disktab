# Leave nc=16; adjust size using: ns
rdroot|ramdiskroot|RAM-disk root FS image:\
	:dt=rdroot:se#512:nc#16:nt#2:ns#580:\
	:ta=4.2BSD:oa#0:pa#18560:fa#512:ba#4096:\
	:ob#0:pb#0:oc#0:pc#18560:

miniroot:\
	:dt=rdroot:se#512:nc#9:nt#16:ns#256:\
	:oc#0:pc#36864:
