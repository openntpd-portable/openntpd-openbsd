        :pa#30294:oa#306:ba#8192:fa#1024:ta=4.2BSD:\
	:pb#61200:ob#30600:tb=swap:\
	:pc#326808:oc#0:\
	:ph#235008:oh#91800:bh#8192:fh#1024:th=4.2BSD:

