console	"/usr/libexec/getty std.9600"	vt220	on   secure
# on-board serial ports (ttyb is the mouse port)
ttya	"/usr/libexec/getty std.9600"	unknown	off  secure	# console
ttyc	"/usr/libexec/getty std.9600"	unknown	off
