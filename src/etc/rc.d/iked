#!/bin/ksh
#
# $OpenBSD: iked,v 1.5 2018/01/11 21:09:26 rpe Exp $

daemon="/sbin/iked"

. /etc/rc.d/rc.subr

pexp="iked: parent.*"

rc_pre() {
	[[ ${sasyncd_flags} != NO ]] &&
		daemon_flags="-S ${daemon_flags}"
	#return 0
	# child will not return a config parsing error to the parent
	${daemon} -n ${daemon_flags}
}

rc_cmd $1
